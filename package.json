{
  "name": "react-jed",
  "version": "0.0.1",
  "description": "React i18n wrapper for jed, based on jed",
  "author": "App Annie Engineering",
  "files": [
    "lib",
    "index.js"
  ],
  "main": "index.js",
  "keywords": [
    "react-jed",
    "jed",
    "gettext",
    "i18n",
    "internationalization",
    "localization"
  ],
  "dependencies": {
    "fetch-ponyfill": "^4.1.0",
    "hoist-non-react-statics": "^2.3.0",
    "jed": "^1.1.1",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "universal-cookie": "^2.0.8"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^2.9.1",
    "eslint": "^4.1.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-prettier": "^2.3.0",
    "eslint-config-xo-space": "^0.16.0",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-plugin-react": "^7.3.0",
    "fetch-ponyfill": "^4.1.0",
    "husky": "^0.14.3",
    "jest": "^19.0.2",
    "jest-cli": "^20.0.0",
    "lint-staged": "^4.0.3",
    "nsp": "^2.6.3",
    "prettier": "^1.5.3",
    "react-test-renderer": "^15.6.1"
  },
  "scripts": {
    "test": "jest --coverage",
    "prepublish": "nsp check",
    "pretest": "eslint .",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "repository": "appannie/react-jed",
  "license": "MIT",
  "eslintConfig": {
    "extends": [
      "xo-space",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "indent": "off",
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "trailingComma": "es5",
          "tabWidth": 4,
          "printWidth": 90
        }
      ]
    }
  },
  "jest": {
    "testEnvironment": "node",
    "coverageReporters": [
      "lcov",
      "text",
      "html"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  }
}
